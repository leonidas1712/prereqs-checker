{
  "name": "prereq-ext",
  "version": "0.0.1",
  "description": "Chrome extension to assist in checking prerequisites while viewing nusmods.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/huzaifa1712/prereqs-checker"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:watch": "cross-env __DEV__=true vite build -w",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "npm run build:hmr && (run-p wss build:watch)",
    "test": "npx vitest run"
  },
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^6.0.17",
    "@mantine/hooks": "^6.0.17",
    "@mantine/notifications": "^6.0.17",
    "fp-ts": "^2.16.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "13.4.0",
    "@types/chrome": "0.0.224",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.17.1",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.2.4",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.38.1",
    "@vitejs/plugin-react": "2.2.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "axios-retry": "^3.6.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "eslint": "8.36.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "fs-extra": "11.1.0",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "2.8.8",
    "react-icons": "^4.10.1",
    "rollup": "2.79.1",
    "sass": "1.62.1",
    "tailwindcss": "^3.3.2",
    "ts-jest": "29.0.2",
    "ts-loader": "9.4.2",
    "typescript": "4.8.3",
    "vite": "^3.1.3",
    "vitest": "^0.33.0",
    "ws": "8.13.0"
  }
}
